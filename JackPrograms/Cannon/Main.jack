// Based on the Square Dance game
// Creates a cannon game and starts running it

/** Initializes a new Cannon game and starts running it. */
class Main {
    function void main() {
        var CannonGame game;
        let game = CannonGame.new();
        do game.run();
        do game.dispose();
        return;
    }
}
